<UserControl x:Class="dnSpy.ScyllaHide.ScyllaHideSettingsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
            xmlns:wpfLogicAndUi="clr-namespace:WpfLogicAndUi;assembly=WpfLogicAndUi"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <CheckBox Content="Enabled" IsChecked="{Binding IsEnabledOption}" />
    <wpfLogicAndUi:SettingsPageLayout></wpfLogicAndUi:SettingsPageLayout>
    </StackPanel>

</UserControl>

<UserControl x:Class="Example1.Extension.MySettingsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Example1.Extension"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <CheckBox Content="Enabled" IsChecked="{Binding BoolOption1}" Checked="CheckBox_Checked" />
        <GroupBox Header="Profiles" >
            <Grid>
                <Label Content="Loaded" Margin="0,0,0,0" RenderTransformOrigin="0.567,-0.16"/>
                <ComboBox Height="23" Width="230" Margin="50,0,0,0" SelectedIndex="0">
                    <ListBoxItem Content="Themida"/>
                    <ListBoxItem Content="VMProtect"/>
                </ComboBox>
            </Grid>
        </GroupBox>
    </StackPanel>
</UserControl>
